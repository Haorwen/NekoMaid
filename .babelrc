{
  "plugins": [
    [
      "babel-plugin-transform-imports",
      {
        "@mui/material": {
          "transform": "@mui/material/${member}",
          "preventFullImport": true
        },
        "@mui/icons-material": {
          "transform": "@mui/icons-material/${member}",
          "preventFullImport": true
        },
        "@mui/lab": {
          "transform": "@mui/lab/${member}",
          "preventFullImport": true
        },
        "mdi-material-ui": {
          "transform": "mdi-material-ui/${member}",
          "preventFullImport": true
        }
      }
    ]
  ]
}

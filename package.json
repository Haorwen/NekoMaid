{
  "name": "nekomaid",
  "version": "0.0.0",
  "description": "A plugin can use Web to manage your server.",
  "main": "index.js",
  "scripts": {
    "test": "eslint",
    "start": "parcel web/index.html",
    "generateIcons": "npm run generateMaterialIcons && npm run generateMinecraftIcons",
    "generateMaterialIcons": "ts-node scripts/generateMaterialIcons.ts",
    "generateMinecraftIcons": "ts-node scripts/generateMinecraftIcons.ts",
    "build": "parcel build web/index.html --public-url . --no-source-maps"
  },
  "targets": {
    "main": false
  },
  "engines": {
    "browsers": "Chrome 69"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neko-craft/NekoMaid.git"
  },
  "keywords": [
    "minecraft"
  ],
  "author": "Shirasawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neko-craft/NekoMaid/issues"
  },
  "homepage": "https://github.com/neko-craft/NekoMaid#readme",
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "vscode-material-icon-theme/icons",
        "staticOutDir": "dist/icons/material"
      },
      {
        "staticPath": "icons",
        "staticOutDir": "dist/icons"
      }
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/roboto": "^4.5.0",
    "@fontsource/roboto-mono": "^4.5.0",
    "@material-ui/core": "^5.0.0-beta.0",
    "@material-ui/data-grid": "^4.0.0-alpha.33",
    "@material-ui/icons": "^5.0.0-beta.0",
    "@material-ui/lab": "^5.0.0-alpha.39",
    "@material-ui/styles": "^5.0.0-beta.0",
    "@types/yauzl": "^2.9.2",
    "codemirror": "^5.62.0",
    "dayjs": "^1.10.6",
    "echarts": "^5.1.2",
    "echarts-for-react": "^3.0.1",
    "lodash": "^4.17.21",
    "material-ui-cron": "^0.1.0-beta.1",
    "react": "^17.0.2",
    "react-codemirror2": "^7.2.1",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "skinview3d": "^2.0.1",
    "socket.io-client": "^4.1.2",
    "typescript": "^4.3.5",
    "valid-filename": "^3.1.0"
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-nightly.2400",
    "@parcel/transformer-less": "^2.0.0-nightly.776",
    "@types/lodash": "^4.14.171",
    "@types/react": "^17.0.14",
    "@types/react-copy-to-clipboard": "^5.0.1",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/request": "^2.48.6",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "nugget": "^2.0.1",
    "parcel": "^2.0.0-nightly.776",
    "parcel-reporter-copystatic": "^1.1.0",
    "ts-node": "^10.1.0",
    "yauzl": "^2.10.0"
  }
}
